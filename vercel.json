{
    "builds": [
        {
        "src": "project/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.13.2"
        }
        },
        {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
        }
    ],
    "routes": [
        {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
        },
        {
        "src": "/(.*)",
        "dest": "project/wsgi.py"
        }
    ]
}
